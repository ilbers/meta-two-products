# LAYER_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
LCONF_VERSION = "6"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ##ISARROOT##/meta \
  ##ISARROOT##/meta-two-products/meta-common \
  ##ISARROOT##/meta-two-products/meta-product-a \
  ##ISARROOT##/meta-two-products/meta-product-b \
  "
BBLAYERS_NON_REMOVABLE ?= " \
  ##ISARROOT##/meta \
  "
